{
  "formatVersion":"2.0",
  "appName":"myapp",
  "appVersion":"2.3-SNAPSHOT2",
  "cluster":{
    "version":"emr-6.2.0",
    "serviceAccessSecurityGroup":"sg-aaaaaaaa",
    "ec2SubnetId":"subnet-aaaaaaaa",
    "emrManagedMasterSecurityGroup":"sg-aaaaaaaa",
    "emrManagedSlaveSecurityGroup":"sg-aaaaaaaa",
    "nodes":[
      {
        "role":"MASTER",
        "instanceType":"m5.xlarge",
        "instanceCount":1,
        "volumes":[
          {
            "volumeType":"gp2",
            "sizeInGb":200
          }
        ]
      },
      {
        "role":"CORE",
        "instanceType":"c5.2xlarge",
        "instanceCount":2,
        "volumes":[
          {
            "volumeType":"gp2",
            "sizeInGb":200
          }
        ]
      },
      {
        "role":"TASK",
        "instanceType":"c5.2xlarge",
        "instanceCount":4,
        "volumes":[
          {
            "volumeType":"gp2",
            "sizeInGb":32
          }
        ]
      }
    ],
    "tags":[
      {
        "name":"CustomApplicationEnvironment",
        "value":"Development"
      },
      {
        "name":"CustomApplicationTeam",
        "value":"MyAppName"
      },
      {
        "name":"Owner",
        "value":"MyAppOwner"
      }
    ]
  },
  "bootstrapScripts":[

  ],
  "appConf":{
    "appType":"spark",
    "driverMemory":"4g",
    "conf":[
      "spark.sql.shuffle.partitions=96",
      "spark.dynamicAllocation.enabled=true",
      "spark.dynamicAllocation.executorIdleTimeout=1800s",
      "spark.driver.defaultJavaOptions=-XX:+UseG1GC",
      "spark.executor.defaultJavaOptions=-XX:+UseG1GC",
      "spark.streaming.stopGracefullyOnShutdown=true",
      "spark.yarn.maxAppAttempts=1",
      "spark.eventLog.enabled=false"
    ],
    "packages":[
      "com.qubole.spark:spark-sql-kinesis_2.12:1.2.0_spark-3.0"
    ],
    "extraClasspath":{
      "folder":"/home/hadoop/external_libs/",
      "jarUrls":[
        "https://repo1.maven.org/maven2/mysql/mysql-connector-java/8.0.20/mysql-connector-java-8.0.20.jar"
      ]
    },
    "args":[

    ]
  }
}